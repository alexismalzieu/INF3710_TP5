<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Article" table="Article">


		<id name="soumisLe" column="soumisLe" type="date" />

		<many-to-one name="auteur" class="model.Chercheur" fetch="join"
			update="false" insert="false">
			<column name="matricule" />
		</many-to-one>

		<many-to-one name="coauteur" class="model.Chercheur"
			fetch="join" update="false" insert="false">
			<column name="matricule" />
		</many-to-one>

		<many-to-one name="departement" class="model.Departement"
			fetch="join">
			<column name="nom" />
		</many-to-one>

	</class>
</hibernate-mapping>